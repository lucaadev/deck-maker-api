  "dependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "mysql2": "^3.6.5",
    "node-typescript": "^0.1.3",
    "nodemon": "^3.0.1",
    "sequelize": "^6.0.0",
